{
  "version": 2,
  "builds": [
    {
      "src": "NUCLEUS-VERCEL/nucleus/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public/static"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "public/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "NUCLEUS-VERCEL/nucleus/wsgi.py"
    }
  ]
}
